Config { font = "xft:Fixed-8"
    , bgColor = "#000000"
    , fgColor = "#ffffff"
    , position = Static { xpos = 0, ypos = 0, width = 1920, height = 16 }
    , lowerOnStart = True
    commands =	[ Run MultiCpu ["-t","CPU: <total0> <total1> <total2> <total3>"
        	,"-L"	,"30"
		,"-H"	,"60"
		,"-l"	,"#15FF00"
		,"-n"	,"#FF6600"
		,"-h"	,"#FF0000"
		,"-w"	,"3"] 10
	, Run CoreTemp ["-t","Temp: <core0>째C <core1>째C <core2>째C <core3>째C"
		,"-L"	,"60"
		,"-H"	,"80"
		,"-l"	,"#15FF00"
		,"-n"	,"#FF6600"
		,"-h"	,"#FF0000"] 50
	, Run Memory ["-t","Mem: <usedratio>%"
		,"-L"	,"25"
		,"-H"	,"75"
		,"-l"	,"#15FF00"
		,"-n"	,"#FF6600"
		,"-h"	,"#FF0000"] 10
	, Run Swap ["-t","Swap: <usedratio>%"
		,"-L"	,"25"
		,"-H"	,"75"
		,"-l"	,"#15FF00"
		,"-n"	,"#FF6600"
		,"-h"	,"#FF0000"] 10
	, Run Network "eno1" ["-t","eno1: <rx> <tx>"
		,"-L"	,"0"
		,"-H"	,"50000000"
		,"-n"	,"#15FF00"
		,"-h"	,"#FF0000"
		,"-S"	,"true"] 10
        , Run StdinReader
        , Run Date "%a %b %_d %T" "date" 10
    ]
    , sepChar = "%"
    , alignSep = "}{"
    , template = "%StdinReader% }{ %eno1% | %multicpu% | %coretemp% | %memory% | %swap% | <fc=#FFFFCC>%date%</fc>"
}
