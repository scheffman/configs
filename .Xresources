! vim: set filetype=xdefaults :
 
URxvt.perl-ext-common : default,keyboard-select,url-select,clipboard
 
URxvt.keysym.Alt-x           : perl:keyboard-select:activate
URxvt.keysym.Alt-u           : perl:url-select:select_next
URxvt.keysym.Alt-c           : perl:clipboard:copy
URxvt.keysym.Alt-v           : perl:clipboard:paste
URxvt.keysym.Alt-s           : perl:keyboard-select:search
URxvt.keysym.Alt-C-v         : perl:clipboard:paste_escaped
URxvt.url-select.launcher  : dwb
URxvt.url-select.autocopy  : true
URxvt.url-select.underline : true
 
URxvt.geometry  : 119x45
 
URxvt.font        : xft:Dejavu Sans Mono:pixelsize=13
URxvt.letterSpace : 0
URxvt.termName    : rxvt-256color
URxvt.iso14755    : false
 
URxvt.urgentOnBell: true
 
URxvt.saveLines   : 12000
URxvt.scrollBar   : false
URxvt.scrollstyle : rxvt
 
!XFT
Xft.lcdfilter : lcddefault
Xft.hintstyle : hintslight
Xft.hinting   : true
Xft.antialias : true
Xft.rgba      : rgb
 
!Solarized + tangerine + random colours
URxvt.cursorColor            : #FF0000
URxvt*cursorBlink:      true
 
URxvt.pointerColorBackground : #586E7a5
URxvt.urlLauncher:     /usr/bin/dwb
URxvt.pointerColorForeground : #00FF00
 
URxvt.Depth : 32
URxvt.background : #000000
URxvt.foreground : #FFFFFF
URxvt*.transparent  : True
URxvt*.shading: 70
 
URxvt.colorBD : #FFFFFF
URxvt.colorUL : #CCCC66
 
